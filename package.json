    "@ton/ton": "^14.0.0",
    "@tonconnect/ui-react": "^2.0.5",
    "axios": "^1.7.2",
    "eruda": "^3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.23.0",
    "react-timer-hook": "^3.0.7",
